{
  "name": "@t-bowersox/container",
  "description": "A dependency injection container with a simple API.",
  "version": "1.0.1",
  "main": "dist/common/index.js",
  "module": "dist/esm/index.js",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/common/index.js"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/t-bowersox/container.git"
  },
  "keywords": [
    "container",
    "dependency injection",
    "typescript",
    "DI",
    "ioc"
  ],
  "author": "t-bowersox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/t-bowersox/container/issues"
  },
  "homepage": "https://github.com/t-bowersox/container#readme",
  "type": "module",
  "devDependencies": {
    "@babel/core": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@types/jest": "^27.4.0",
    "@typescript-eslint/eslint-plugin": "^5.9.0",
    "@typescript-eslint/parser": "^5.9.0",
    "babel-jest": "^27.4.5",
    "eslint": "^8.6.0",
    "eslint-config-prettier": "^8.3.0",
    "jest": "^27.4.5",
    "prettier": "^2.5.1",
    "ts-jest": "^27.1.2",
    "typescript": "^4.5.4"
  },
  "scripts": {
    "watch": "tsc --watch",
    "build": "tsc && tsc --project tsconfig.common.json && npm run package:common && npm run package:esm",
    "lint": "eslint src",
    "format": "prettier --write src",
    "test": "jest",
    "test:watch": "jest --watch",
    "package:common": "cp package.common.json dist/common/package.json",
    "package:esm": "cp package.esm.json dist/esm/package.json"
  },
  "files": [
    "dist"
  ]
}
